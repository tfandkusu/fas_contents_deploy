{
  "hosting": [
    {
      "target": "contents",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "headers": [ 
        {
          "source": "**/*",
          "headers": [
            {
              "key": "Access-Control-Allow-Origin",
              "value": "https://flutter-architecture-sample.web.app"
            }
          ]
        }
      ]
    }
  ]
}
